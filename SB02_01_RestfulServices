package com.codex.sb02_journal.model;

import java.util.Date;

public class journalModel {
    private String title;
    private Date date;
    private String text;
    private boolean isPinned;

    public journalModel(String title, Date date, String text, boolean isPinned) {
        this.title = title;
        this.date = date;
        this.text = text;
        this.isPinned = isPinned;
    }

    @Override
    public String toString() {
        return "journalModel{" +
                "title='" + title + '\'' +
                ", date=" + date +
                ", text='" + text + '\'' +
                ", isPinned=" + isPinned +
                '}';
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public Date getDate() {
        return date;
    }

    public void setDate(Date date) {
        this.date = date;
    }

    public String getText() {
        return text;
    }

    public void setText(String text) {
        this.text = text;
    }

    public boolean isPinned() {
        return isPinned;
    }

    public void setPinned(boolean pinned) {
        isPinned = pinned;
    }
}


package com.codex.sb02_journal.service;

import com.codex.sb02_journal.model.journalModel;

import java.util.ArrayList;
import java.util.Date;

public class journalService {

    private ArrayList<journalModel> journals = new ArrayList<journalModel>();

    protected void addJournal (String title, Date date, String text, boolean isPinned){
        journals.add(new journalModel(title, date, text, isPinned));
    }

    protected void showJournals(){
        for (journalModel journal : journals) {
            System.out.println(journal);
        }
    }

}



