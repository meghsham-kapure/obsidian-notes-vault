        j1.setJournalTitle("Learn Spring Boot");
        j1.setJournalDate("11 April 2025");
        j1.setJournalDescription("Hey I am learning spring boot application");
        j1.setJournalType("Spring Boot");
        j1.setJournalLocation("Karvenagar, Pune");
        j1.setJournalStatus("Success");



        Journal j2 = new Journal();
        j2.setJournalId(2);
        j2.setJournalTitle("Learn Spring Boot");
        j2.setJournalDate("12 April 2025");
        j2.setJournalDescription("Hey I am building spring boot application");
        j2.setJournalType("Spring Boot");
        j2.setJournalLocation("Kalyani Nagar, Pune");
        j2.setJournalStatus("Success");



        Journal j3 = new Journal();
        j3.setJournalId(3);
        j3.setJournalTitle("Learn React");
        j3.setJournalDate("13 April 2025");
        j3.setJournalDescription("Hey I am learning react spa application");
        j3.setJournalType("React jS");
        j3.setJournalLocation("Karvenagar, Pune");
        j3.setJournalStatus("Failed");



        Journal j4 = new Journal();
        j4.setJournalId(4);
        j4.setJournalTitle("Build React");
        j4.setJournalDate("14 April 2025");
        j4.setJournalDescription("Hey I am building react spa application");
        j4.setJournalType("React jS");
        j4.setJournalLocation("Kalyani Nagar, Pune");
        j4.setJournalStatus("Success");

        System.out.println(j1);
        System.out.println(j2);
        System.out.println(j3);
        System.out.println(j4);

        System.out.println(j);


    }

}

